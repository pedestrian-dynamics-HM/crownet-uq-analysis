#!/bin/bash
base="$PWD"
echo "#######################################"
echo $base
echo "#######################################"

echo "Pull git repos"

echo "#######################################"
echo "#######################################"
echo "#######################################"
echo "Pull rover-main"
pushd "$base/rover-main"
git pull
git checkout "4c0f84d1f8a81ba959ed53330050e1493cbddaeb"
git submodule update --recursive

echo "Pull suqc"
pushd "$base/suq-controller"
git pull
git checkout "b1c87aaf5fff99927e050f1595978ead5b3a1db9"

echo "#######################################"
echo "#######################################"
echo "#######################################"
